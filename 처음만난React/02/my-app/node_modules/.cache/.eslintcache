ent} = this.props;\n\n        return (\n            <div style={styles.root}>\n                <div style={styles.imageContainer}>\n                    <img \n                        src='https://upload.wikimedia.org/wikipedia/commons/8/89/Portrait_Placeholder.png'\n                        style={styles.image}/>\n                </div>\n                <div style={styles.commentContainer}>\n                    <div style={styles.nameText}>\n                        {/* {'Serin Kim'} */}\n                        {/* {this.props.name} */}\n                        {name};\n                    </div>\n                    <span style={styles.contentText}>\n                        {/* {'My comment!'} */}\n                        {/* {this.props.content} */}\n                        {content}\n                    </span> \n                </div>\n\n            </div>\n        )\n    }\n}\n\nexport default Comment;","/Users/serinkim/Documents/FEstudy/처음만난React/02/my-app/src/Chapter06.js",[],"/Users/serinkim/Documents/FEstudy/처음만난React/02/my-app/src/Chapter07/Greeting.js",[],"/Users/serinkim/Documents/FEstudy/처음만난React/02/my-app/src/Chapter07/LoginControl.js",[],{"ruleId":"41","severity":1,"message":"42","line":4,"column":8,"nodeType":"43","messageId":"44","endLine":4,"endColumn":11},{"ruleId":"41","severity":1,"message":"45","line":5,"column":8,"nodeType":"43","messageId":"44","endLine":5,"endColumn":17},{"ruleId":"41","severity":1,"message":"46","line":7,"column":8,"nodeType":"43","messageId":"44","endLine":7,"endColumn":16},{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"41","severity":1,"message":"51","line":6,"column":7,"nodeType":"43","messageId":"44","endLine":6,"endColumn":25},{"ruleId":"52","severity":1,"message":"53","line":42,"column":5,"nodeType":"54","messageId":"55","endLine":46,"endColumn":6},{"ruleId":"56","severity":1,"message":"57","line":66,"column":21,"nodeType":"58","endLine":68,"endColumn":47},"no-unuse